{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/app/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/app/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/app",
  "projects": {
    "auth": {
      "type": "application",
      "root": "apps/auth",
      "entryFile": "main",
      "sourceRoot": "apps/auth/src",
      "compilerOptions": {
        "tsConfigPath": "apps/auth/tsconfig.app.json"
      }
    },
    "app": {
      "type": "application",
      "root": "apps/app",
      "entryFile": "main",
      "sourceRoot": "apps/app/src",
      "compilerOptions": {
        "tsConfigPath": "apps/app/tsconfig.app.json"
      }
    },
    "payment": {
      "type": "application",
      "root": "apps/payment",
      "entryFile": "main",
      "sourceRoot": "apps/payment/src",
      "compilerOptions": {
        "tsConfigPath": "apps/payment/tsconfig.app.json"
      }
    },
    "personnalisation-graphique": {
      "type": "application",
      "root": "apps/personnalisation-graphique",
      "entryFile": "main",
      "sourceRoot": "apps/personnalisation-graphique/src",
      "compilerOptions": {
        "tsConfigPath": "apps/personnalisation-graphique/tsconfig.app.json"
      }
    },
    "produits": {
      "type": "application",
      "root": "apps/produits",
      "entryFile": "main",
      "sourceRoot": "apps/produits/src",
      "compilerOptions": {
        "tsConfigPath": "apps/produits/tsconfig.app.json"
      }
    },
    "shop": {
      "type": "application",
      "root": "apps/shop",
      "entryFile": "main",
      "sourceRoot": "apps/shop/src",
      "compilerOptions": {
        "tsConfigPath": "apps/shop/tsconfig.app.json"
      }
    },
    "users": {
      "type": "application",
      "root": "apps/users",
      "entryFile": "main",
      "sourceRoot": "apps/users/src",
      "compilerOptions": {
        "tsConfigPath": "apps/users/tsconfig.app.json"
      }
    }
  }
}