{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/nest-e_commerce-app/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/nest-e_commerce-app/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/nest-e_commerce-app",
  "projects": {
    "auth": {
      "type": "application",
      "root": "apps/auth",
      "entryFile": "main",
      "sourceRoot": "apps/auth/src",
      "compilerOptions": {
        "tsConfigPath": "apps/auth/tsconfig.app.json"
      }
    },
    "nest-e_commerce-app": {
      "type": "application",
      "root": "apps/nest-e_commerce-app",
      "entryFile": "main",
      "sourceRoot": "apps/nest-e_commerce-app/src",
      "compilerOptions": {
        "tsConfigPath": "apps/nest-e_commerce-app/tsconfig.app.json"
      }
    },
    "users": {
      "type": "application",
      "root": "apps/users",
      "entryFile": "main",
      "sourceRoot": "apps/users/src",
      "compilerOptions": {
        "tsConfigPath": "apps/users/tsconfig.app.json"
      }
    }
  }
}